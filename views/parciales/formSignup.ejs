<form class="form" action="/signup"  method="post" role="form" autocomplete="off">
    <fieldset>
      <legend>Registrarse</legend>
      <div class="mb-3">
        <label for="titulo" class="form-label">Usuario</label>
        <input required name="usuario" type="text" id="disabledTextInput" class="form-control titulo" placeholder="Ingrese el usuario..">
      </div>
      <div class="mb-3">
        <label for="titulo" class="form-label">Contraseña</label>
        <input required name="contrasena" type="password" id="precio" class="form-control precio" placeholder="Ingrese la contraseña..">
      </div>
      <div class="mb-3">
        <label for="email" class="form-label">Email</label>
        <input required name="email" type="email" id="precio" class="form-control precio" placeholder="Ingrese su email..">
      </div>
      <div class="mb-3">
        <label for="age" class="form-label">Edad</label>
        <input required name="age" type="number" id="precio" class="form-control precio" placeholder="Ingrese su edad...">
      </div>
      <div class="mb-3">
        <label for="celphone" class="form-label">Celular</label>
        <input required name="celphone" type="number" id="precio" class="form-control precio" placeholder="Ingrese su celular...">
      </div>
      <button type="submit" class="btn btn-primary">Enviar</button>
      <a class="btn btn-danger" href="/login"><span class="fa fa-sign-out"></span><span>Login</span></a>
    </fieldset>
  </form>